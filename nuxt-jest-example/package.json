        ]
      }
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "nuxt": "^2.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "lodash.clonedeep": "^4.5.0",
    "nodemon": "^1.18.9"
  }
}
